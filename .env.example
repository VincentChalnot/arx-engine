# Example .env file for production deployment
# Copy this file to .env and customize for your environment

# Environment identifier (production, staging, development)
ENVIRONMENT=production

# Docker image references (automatically set by CI/CD)
FRONTEND_IMAGE=ghcr.io/vincentchalnot/arx-engine-frontend:latest
BACKEND_IMAGE=ghcr.io/vincentchalnot/arx-engine-backend:latest

# Domain configuration
FRONTEND_DOMAIN=arx.example.com
BACKEND_DOMAIN=api.arx.example.com

# Backend configuration
RUST_LOG=info
